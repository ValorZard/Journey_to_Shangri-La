		{
			"character": "character-1676265223.json",
			"event_id": "dialogic_001",
			"portrait": "Sad_Professor",
			"text": "Yes, you're right."
		},
		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "He tears up a bit and wipes his eyes."
		},
		{
			"character": "character-1676265223.json",
			"event_id": "dialogic_001",
			"portrait": "Sad_Professor",
			"text": "There's no use in complaining - I am a man of action after all!"
