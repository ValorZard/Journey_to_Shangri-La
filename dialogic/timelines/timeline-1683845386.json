		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Mateo smiles."
		},
		{
			"character": "character-1683825918.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Alrighty, if you insist."
		},
		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "You and Mateo share."
		},
		{
			"event_id": "dialogic_013"
