			"type": 0,
			"z_index": 0
		},
		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "You follow the source of the sound and discover Mateo, curled into a corner on the floor and strumming the guitar that usually hangs on his hip. "
		},
		{
			"character": "",
			"event_id": "dialogic_010",
			"options": [

			],
			"portrait": "",
			"question": "His brows are furrowed with concentration."
		},
		{
			"choice": "Walk up to him.",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
		{
			"character": "character-1676265208.json",
			"event_id": "dialogic_001",
			"portrait": "MC_Happy",
			"text": "I always see you with that thing."
		},
		{
			"animation": "shake_y.gd",
			"animation_length": 1,
			"change_mirror_portrait": false,
			"change_position": false,
