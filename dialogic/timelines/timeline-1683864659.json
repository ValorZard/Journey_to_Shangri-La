				"3": false,
				"4": false
			},
			"type": 2,
			"z_index": 0
		},
		{
			"character": "character-1683825918.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Alright, if you feel more comfortable like that."
		},
		{
			"choice": "“Yeah!”",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
