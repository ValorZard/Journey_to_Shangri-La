		},
		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Mateo jerks sharply like a startled animal. "
		},
		{
			"animation": "[No Animation]",
			"animation_length": 0,
			"change_mirror_portrait": true,
			"change_position": false,
			"change_z_index": false,
			"character": "character-1683825918.json",
			"event_id": "dialogic_002",
			"mirror_portrait": false,
			"portrait": "(Don't change)",
